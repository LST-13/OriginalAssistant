<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CommonApp" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryVariant">@color/DeepGray</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/DeepSkyBlue</item>
        <item name="colorSecondaryVariant">@color/SkyBlue</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->

        <item name="DayColorPrimaryNightWhite">@color/white</item>
        <item name="DayWhiteNightBlack">@color/black</item>

        <item name="EditTextCursorColor">@color/white</item>
        <item name="android:textCursorDrawable">@drawable/edit_text_cursor</item>

        <item name="FloatingActionButtonLabelBgColor">#28ffffff</item>
        <item name="FloatingActionButtonNormalColor">#444444</item>
        <item name="FloatingActionButtonPressedColor">#222222</item>
        <item name="RippleTransparentColor">#21F5F5F5</item>
    </style>
</resources>